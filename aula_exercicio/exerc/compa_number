//funçãp que recebe 2 números e retorna o maior deles

const min = 1;
const max = 50

const random = (min, max) => {
    const r = Math.random() * (max - min) + min;
    return Math.floor(r);
}

const x = random(min, max);
const y = random(min, max);

const comparNumber = (x,y) => x > y ? x :y;

const result = comparNumber(x, y);

console.log(result)
